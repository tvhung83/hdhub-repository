<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.hdhub.com"
		name="HDhub Add-on Repository"
		provider-name="hdhub">
	<requires>
		<import addon="xbmc.addon" version="12.0.0"/>
	</requires>
	<extension point="xbmc.addon.repository"
		name="HDhub Add-on Repository">
		<info compressed="true">https://raw.github.com/tvhung83/hdhub-repository/master/dist/addons.xml</info>
		<checksum>https://raw.github.com/tvhung83/hdhub-repository/master/dist/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.github.com/tvhung83/hdhub-repository/master/dist</datadir>
		<hashes>true</hashes>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">HDhub - HD for all</summary>
		<description lang="en"></description>
		<disclaimer lang="en"></disclaimer>
		<platform>all</platform>
	</extension>
</addon>